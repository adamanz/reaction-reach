# ReactionReach Environment Configuration

# Browserbase Configuration (Required)
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# OpenAI Configuration (Required for Stagehand)
OPENAI_API_KEY=your_openai_api_key_here

# LinkedIn Configuration
LINKEDIN_PROFILE_URL=https://linkedin.com/in/your-profile
TARGET_DAYS_BACK=30

# LinkedIn Authentication (Required for accessing profiles and posts)
LINKEDIN_USERNAME=your_linkedin_email@example.com
LINKEDIN_PASSWORD=your_linkedin_password

# Authentication & Session Management
USE_STORED_COOKIES=true
SESSION_STORAGE_PATH=./data/linkedin_session.json
ENABLE_PROXIES=true
ENABLE_STEALTH_MODE=true

# Optional: Advanced Configuration
STAGEHAND_VERBOSE=1
MAX_POSTS_TO_ANALYZE=50
DELAY_BETWEEN_POSTS=3000
DELAY_BETWEEN_REACTIONS=1500

# Optional: CrewAI Configuration
CREWAI_VERBOSE=true
CREWAI_MAX_RPM=10

# Optional: Data Storage
OUTPUT_DIR=./data
SAVE_RAW_DATA=true
GENERATE_MARKDOWN_REPORT=true

# Development/Debug Settings
DEBUG_MODE=false
ENABLE_DEBUG_URLS=false
MANUAL_AUTH_REQUIRED=true

# Weights & Biases Configuration
WANDB_PROJECT=reaction-reach
WANDB_ENTITY=your-wandb-username
WANDB_API_KEY=your-wandb-api-key
WANDB_LOG_MODEL=true
WANDB_SAVE_CODE=true